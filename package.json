{
  "name": "next-starter-kit",
  "version": "1.0.0",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "build": "npm run lint && next build",
    "build-static": "npm run lint && next build && next export",
    "dev": "next",
    "export": "rm -rf .next && npm run build && next export",
    "lint": "eslint --config .eslintrc.js ./components ./pages --ext .ts,.tsx,.js --fix",
    "lint-staged": "lint-staged --config .lintstagedrc.json",
    "now": "now",
    "prettier": "prettier --config .prettierrc.json --write \"{pages,components}/**/*.{ts,tsx}\"",
    "start": "next start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@emotion/babel-preset-css-prop": "10.0.27",
    "@emotion/core": "10.0.27",
    "@types/react-transition-group": "4.2.3",
    "@typescript-eslint/eslint-plugin": "2.16.0",
    "@typescript-eslint/parser": "2.16.0",
    "babel-plugin-emotion": "10.0.27",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-config-standard": "14.1.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "eslint-plugin-simple-import-sort": "5.0.0",
    "eslint-plugin-standard": "4.0.1",
    "husky": "3.1.0",
    "lint-staged": "9.2.5",
    "next": "9.2.0",
    "now": "16.7.3",
    "prettier": "1.19.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-transition-group": "4.3.0",
    "ts-node": "8.6.2",
    "typescript": "3.7.5"
  }
}
