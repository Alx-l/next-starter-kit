{
  "name": "next-starter-kit",
  "version": "1.0.0",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "build": "npm run lint:prod && next build",
    "build-static": "npm run lint:prod && next build && next export",
    "dev": "next",
    "export": "rm -rf .next && npm run build && next export",
    "lint": "eslint --config .eslintrc.js ./src --ext .ts,.tsx,.js --fix",
    "lint:prod": "eslint --config .eslintrc.prod.js ./src --ext .ts,.tsx,.js --fix",
    "lint:hook": "eslint --config .eslintrc.prod.js --fix",
    "lint-staged": "lint-staged --config .lintstagedrc.json",
    "vercel": "vercel --prod",
    "prettier": "prettier --config .prettierrc.json --write \"src/**/*.{ts,tsx}\"",
    "prettier:hook": "prettier --config .prettierrc.json --write",
    "start": "next start",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm run test --silent -- --coverage",
    "test:coverage:only": "npx jest \"$npm_config_src\" \"--coverage\" \"--collectCoverageFrom='$npm_config_src/**/*.(ts|tsx)'\"",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@babel/core": "7.13.14",
    "@emotion/babel-plugin": "11.2.0",
    "@emotion/react": "11.1.5",
    "@testing-library/jest-dom": "5.11.10",
    "@testing-library/react": "11.2.6",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.37",
    "@emotion/babel-preset-css-prop": "11.0.0",
    "@prefresh/next": "1.4.5",
    "@types/nprogress": "0.2.0",
    "@types/react-transition-group": "4.4.1",
    "@typescript-eslint/eslint-plugin": "4.20.0",
    "@typescript-eslint/parser": "4.20.0",
    "acd-utils": "8.0.0",
    "autoprefixer": "10.2.5",
    "await-to-js": "3.0.0",
    "babel": "6.23.0",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-config-standard": "16.0.2",
    "eslint-loader": "4.0.2",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-standard": "4.1.0",
    "husky": "6.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "10.5.4",
    "next": "10.1.2",
    "nprogress": "0.2.0",
    "preact": "10.5.12",
    "preact-render-to-string": "5.1.12",
    "prettier": "2.2.1",
    "react": "npm:@preact/compat@^0.0.3",
    "react-dom": "npm:@preact/compat@^0.0.3",
    "react-ssr-prepass": "npm:preact-ssr-prepass@^1.1.1",
    "react-transition-group": "4.4.1",
    "ts-jest": "26.5.4",
    "ts-node": "9.1.1",
    "typescript": "4.2.3",
    "vercel": "21.3.3",
    "webpack": "5.28.0"
  },
  "dependencies": {}
}
