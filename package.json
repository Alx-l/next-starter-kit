{
  "name": "next-starter-kit",
  "version": "1.0.0",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "build": "npm run lint && next build",
    "build-static": "npm run lint && next build && next export",
    "dev": "next",
    "export": "rm -rf .next && npm run build && next export",
    "lint": "eslint --config .eslintrc.js ./components ./pages --ext .ts,.tsx,.js --fix",
    "lint-staged": "lint-staged --config .lintstagedrc.json",
    "now": "now",
    "prettier": "prettier --config .prettierrc.json --write \"{pages,components}/**/*.{ts,tsx}\"",
    "start": "next start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@emotion/babel-preset-css-prop": "10.0.17",
    "@emotion/core": "10.0.17",
    "@types/next": "8.0.6",
    "@types/react-transition-group": "4.2.2",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.0.0",
    "babel-plugin-emotion": "10.0.17",
    "eslint": "6.3.0",
    "eslint-config-prettier": "6.1.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-fp": "2.3.0",
    "eslint-loader": "3.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-simple-import-sort": "4.0.0",
    "eslint-plugin-node": "9.2.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "2.0.1",
    "eslint-plugin-standard": "4.0.1",
    "fp-ts": "2.0.5",
    "husky": "3.0.4",
    "lint-staged": "9.2.5",
    "next": "9.0.5",
    "now": "16.1.2",
    "prettier": "1.18.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-transition-group": "4.2.2",
    "ts-node": "8.3.0",
    "typescript": "3.6.2"
  }
}
