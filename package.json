{
  "name": "next-starter-kit",
  "version": "1.0.0",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "build": "npm run lint:prod && next build",
    "build-static": "npm run lint:prod && next build && next export",
    "dev": "next",
    "export": "rm -rf .next && npm run build && next export",
    "lint": "eslint --config .eslintrc.js ./src --ext .ts,.tsx,.js --fix",
    "lint:styles": "stylelint --fix \"./src/**/*.module.css\"",
    "lint:prod": "eslint --config .eslintrc.prod.js ./src --ext .ts,.tsx,.js --fix",
    "lint-staged": "lint-staged --config .lintstagedrc.json",
    "now": "now --prod",
    "prettier": "prettier --config .prettierrc.json --write \"src/**/*.{ts,tsx}\"",
    "start": "next start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@types/node": "14.0.5",
    "@types/nprogress": "0.2.0",
    "@types/react-transition-group": "4.4.0",
    "@typescript-eslint/eslint-plugin": "3.0.1",
    "@typescript-eslint/parser": "3.0.1",
    "acd-utils": "7.1.0",
    "autoprefixer": "9.8.0",
    "await-to-js": "2.1.1",
    "classcat": "4.1.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.3",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "eslint-plugin-standard": "4.0.1",
    "husky": "4.2.5",
    "lint-staged": "10.2.6",
    "next": "9.4.2",
    "now": "19.0.1",
    "nprogress": "0.2.0",
    "postcss-import": "12.0.1",
    "postcss-nesting": "7.0.1",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-transition-group": "4.4.1",
    "stylelint": "13.5.0",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.0.0",
    "tailwindcss": "1.4.6",
    "ts-node": "8.10.1",
    "typescript": "3.8.3"
  },
  "dependencies": {}
}
